@model IEnumerable<MVCDemoTest.threshold>


@{
    ViewBag.Title = "Threshold History";
}


<h2>Threshold History</h2>

<p>
    @Html.ActionLink("Update Examiner Count Threshold", "Threshold")
</p>
<table class="table">
    <tr>
        <th>
            Effective Date
        </th>
        <th>
            Expiry Date
        </th>
        <th>
            Threshold
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.thresh)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.eff_date)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.exp_date)
        </td>
        <td>
            @Html.ActionLink("Edit", "Threshold", new { id=item.id }) |
            @Html.ActionLink("Details", "Details", new { id=item.id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.id })
        </td>
    </tr>
}

</table>
